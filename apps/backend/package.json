{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "author": "Mohammed Gehad",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn build:reflection && tsc",
    "build:reflection": "cross-env NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true NODE_ENV=development ts-node --transpile-only src/schema",
    "start": "cross-env NODE_ENV=production node ./dist",
    "dev": "cross-env ts-node-dev --exit-child --transpile-only src/"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslintconfig": "*",
    "graphql-tag": "^2.12.6",
    "prettier": "^2.6.2"
  }
}
